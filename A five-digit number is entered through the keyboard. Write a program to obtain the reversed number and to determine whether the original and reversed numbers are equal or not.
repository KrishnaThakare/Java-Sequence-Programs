// checking
import java.util.Scanner;
class ReverseNumberAndEqualsToOriginalNumber{
public static void main(String args []){
	Scanner sc = new Scanner(System.in);
	int num,a,b,c,d,e,reverse;
	System.out.println("Enter a 5 digit number");
	num = sc.nextInt();
	a=num%10;	
	b=(num/10)%10;
	c=(num/100)%10;	
	d=(num/1000)%10;
	e=(num/10000)%10;	
	reverse=a*10000 + b*1000 + c*100 + d*10 + e*1;    
	System.out.println("a = " +a);
	System.out.println("b = " +b);
	System.out.println("c = " +c);
	System.out.println("d = " +d);
	System.out.println("e = " +e);
	System.out.println("Reverse number of "+num+" is "+reverse);
	if (num==reverse)
	System.out.println("Both the numbers are equal");
	else
	System.out.println("Both the numbers are not equal");
}
}
