package com.conceptual;

public class Design1 {
	public static void main(String[] args) {
		int i, k;
		for(i=1;i<=1;i++) {
			System.out.print("*****");			
			System.out.println();
			for(k=3;k>=1;k--) {
				System.out.println("*   *");				
			}
		}
		System.out.println("*****");
	}
}
