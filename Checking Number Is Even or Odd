import java.util.Scanner;
class Task4{
public static void main(String args[]){
int num;
Scanner sc = new Scanner(System.in);
System.out.println("Enter numuber to find out even or odd ");
num = sc.nextInt();
if (num % 2==0)
System.out.println("Entered numuber was even number");
else
System.out.println("Entered numuber was odd number");
}
}
