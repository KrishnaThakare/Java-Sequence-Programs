import java.util.Scanner;
public class FobinacciSeries {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int i = 1, a = 1, b = 1, c , n;
		System.out.println("Enter a range");		
		n = sc.nextInt();
		System.out.print(a+" "+b);
		while(i<=n) {
			c = a + b;
			System.out.print(" "+ c);
			a = b;
			b = c;
			i++;
		}	
	}
}
